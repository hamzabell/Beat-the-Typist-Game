) => {\r\n    document.addEventListener(\"keyup\", keyUpHandler);\r\n\r\n    return () => {\r\n      document.removeEventListener(\"keyup\", keyUpHandler);\r\n    };\r\n  }, [keyUpHandler]);\r\n\r\n  const setRandomCharacter = () => {\r\n    const randomInt = Math.floor(Math.random() * 36);\r\n    setCurrentChar(characters[randomInt]);\r\n  };\r\n  return (\r\n    <StyledGame>\r\n      <StyledScore>\r\n        Score: <Strong>{score}</Strong>\r\n      </StyledScore>\r\n      <StyledCharacter>{currentChar}</StyledCharacter>\r\n      <StyledTimer>\r\n        Time:{\" \"}\r\n        <Strong>\r\n          {seconds}:{ms}\r\n        </Strong>\r\n      </StyledTimer>\r\n    </StyledGame>\r\n  );\r\n}\r\n\r\nexport default Game;\r\n","C:\\Users\\akanb\\Documents\\Tutorials\\fullstack-react-demo\\src\\App.js",[],"C:\\Users\\akanb\\Documents\\Tutorials\\fullstack-react-demo\\src\\components\\Navbar.js",["87"],"import React from \"react\";\r\nimport { Link } from \"react-router-dom\";\r\nimport {\r\n  StyledNavBrand,\r\n  StyledNavbar,\r\n  StyledNavItems,\r\n  StyledLink,\r\n  Accent,\r\n} from \"../styled\";\r\nimport { useAuth0 } from \"@auth0/auth0-react\";\r\n\r\nfunction Navbar(props) {\r\n  const { isAuthenticated, loginWithRedirect, logout } = useAuth0();\r\n  return (\r\n    <StyledNavbar>\r\n      <StyledNavBrand>\r\n        <StyledLink to=\"/\">\r\n          Learn.Build.<Accent>Type.</Accent>\r\n        </StyledLink>\r\n      </StyledNavBrand>\r\n      <StyledNavItems>\r\n        <li>\r\n          <StyledLink to=\"/\">Home</StyledLink>\r\n        </li>\r\n        <li>\r\n          <StyledLink to=\"/highScores\">High Scores</StyledLink>\r\n        </li>\r\n        {!isAuthenticated && (\r\n          <li>\r\n            <button onClick={loginWithRedirect}>Login</button>\r\n          </li>\r\n        )}\r\n\r\n        {isAuthenticated && (\r\n          <li>\r\n            <button onClick={logout}>Logout</button>\r\n          </li>\r\n        )}\r\n      </StyledNavItems